<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.inia_mscc.modulos.seg.entidades">
 
  <class name="Usuario" table="tl_seg_usua_usuario">

  	<id name="_id" column="usua_num_id">
			<generator class="native"/>
	</id>
	
	<property name="_login" column="usua_str_login" />
	<property name="_password" column="usua_str_password" />
	<property name="_activado" column="usua_bol_activado" />
	<property name="_ultimoAcceso" column="usua_dte_ultimo_acceso" />
	<property name="_estadoUsuario" column="usua_str_estado_usuario"  />
	  		
  </class>
</hibernate-mapping>