<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="INIA.MSCC.NEGOCIO.SEG.ENTIDADES.Usuario" table="tl_seg_usua_usuario">
		<id name="id" column="usua_num_id" type="long">
			<generator class="native">
			</generator>
		</id>
		<property name="nombre" column="usua_str_nombre" type="java.lang.String" />
		<property name="password" column="usua_str_password" type="java.lang.String" />
		<property name="idDatosUsuario" column="usua_num_id_dato_usuario" type="long" />
		
	</class>
</hibernate-mapping>

