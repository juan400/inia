 <navigation-rule>
  <from-view-id>/login.jsp</from-view-id>
  <navigation-case>
   <from-action>#{sesion.login}</from-action>
   <from-outcome>login-ok</from-outcome>
   <to-view-id>/menuDropDawn.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{sesion.login}</from-action>
   <from-outcome>login-error</from-outcome>
   <to-view-id>/loginFail.jsp</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>registrarse</from-outcome>
   <to-view-id>/menuRich.jsp</to-view-id>
  </navigation-case>
